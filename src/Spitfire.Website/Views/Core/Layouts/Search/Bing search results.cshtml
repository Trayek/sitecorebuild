<!DOCTYPE html>
@using Sitecore.Data.Fields
@using Sitecore.Mvc
@using Sitecore.Data.Items
@using Spitfire.SitecoreExtensions.Extensions;

@model Spitfire.Library.Models.Search.SearcheEngineResults
@{
    string url = "url";
    string url_camp = "camp url";
    string titleText = "Title";
    string shortTitle = "Short Title";
    string keyword = "Keyword";
    if (Model.PromoteItem!=null)
    {
        LinkField lf = (LinkField)Model.PromoteItem.Fields["Title"];
        url = Model.PromoteItem.LinkFieldUrl("Title");
        ReferenceField campfd = Model.PageItem.Fields["campaign"];
        string lf_campaignId = "?sc_camp=" + campfd.TargetID.ToString();
        url_camp = url + lf_campaignId;
        titleText = lf.Text;
        shortTitle = string.Join(" ", titleText.Split(' ').Take(6));
        keyword = Model.PageItem["Keyword"];
    }
}

